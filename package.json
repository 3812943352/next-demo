{
  "name": "next-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .envs/.env.development -- next dev",
    "build": "dotenv -e .envs/.env.production -- next build",
    "start": "dotenv -e .envs/.env.production -- next start",
    "lint": "next lint",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:prettier": "prettier -c --write \"src/**/*\"",
